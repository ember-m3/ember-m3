{
  "name": "ember-m3",
  "version": "0.9.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember test",
    "format": "node_modules/.bin/prettier --write --ignore-path .gitignore 'addon/**/*.js' 'app/**/*.js' 'tests/**/*.js'"
  },
  "files": [
    "package.json",
    "CHANGELOG.md",
    "index.js",
    "LICENSE.md",
    "README.md",
    "UPGRADING.md",
    "addon",
    "app",
    "blueprints",
    "config"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "ember-cli-babel": "^6.3.0"
  },
  "peerDependencies": {
    "ember-data": ">= 3.3.0"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^3.0.0",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~3.3.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^4.0.0",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-pretender": "^3.0.0",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.2",
    "ember-data": "emberjs/data#master",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-lodash": "^4.17.5",
    "ember-qunit-assert-helpers": "^0.2.1",
    "ember-resolver": "^5.0.0",
    "ember-sinon": "^2.0.0",
    "ember-source": "~3.3.0",
    "ember-try": "^1.0.0-beta.2",
    "ember-welcome-page": "^3.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-ember": "^5.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "loader.js": "^4.2.3",
    "prettier": "^1.9.1",
    "testem": "^2.9.0"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "defaultBlueprint": "schema-initializer"
  }
}
