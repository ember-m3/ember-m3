{
  "name": "ember-m3",
  "version": "1.0.0",
  "description": "Alternative to @ember-data/model in which attributes and relationships are derived from API Payloads",
  "keywords": [
    "ember-addon",
    "ember-data",
    "ember-data-addon"
  ],
  "repository": "https://github.com/hjdivad/ember-m3",
  "license": "MIT",
  "scripts": {
    "build": "ember build",
    "format": "eslint --fix .",
    "lint": "yarn lint:js",
    "lint:js": "eslint .",
    "start": "ember server --no-live-reload --port=0",
    "test": "yarn test:ember && yarn test:node",
    "test:ember": "ember test",
    "test:node": "qunit node-tests/run.js"
  },
  "author": "David J. Hamilton <dhamilton@linkedin.com>",
  "contributors": [
    "Dinesh Nalagatla <dinu.nalagatla@gmail.com> (https://www.linkedin.com/in/dineshnalagatla)",
    "Thomas Wang <wangxgwxg@gmail.com> (https://twitter.com/xinganwang)",
    "Sang Mercado <sang.mercado@gmail.com> (http://linkedin.com/in/sangmercado)",
    "Chris Thoburn <runspired@gmail.com> (https://twitter.com/runspired)",
    "Steve Calvert <steve.calvert@gmail.com> (https://www.twitter.com/scalvert)",
    "Stefan Penner <stefan.penner@gmail.com> (https://iamstef.net)",
    "Robert Jackson <me@rwjblue.com> (https://www.rwjblue.com)",
    "Alan Guo",
    "Igor Terzic",
    "Danail Nachev (http://www.linkedin.com/in/danailnachev)",
    "Ibraheem Abdul-Malik"
  ],
  "files": [
    "package.json",
    "CHANGELOG.md",
    "index.js",
    "LICENSE.md",
    "README.md",
    "UPGRADING.md",
    "addon",
    "app",
    "src",
    "blueprints",
    "config"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "babel-plugin-debug-macros": "^0.3.3",
    "broccoli-funnel": "^3.0.1",
    "ember-cli-babel": "^7.18.0",
    "esm": "^3.2.25",
    "semver": "^7.1.3"
  },
  "devDependencies": {
    "@ember-data/debug": "^3.16.0-alpha.3",
    "@ember-data/model": "^3.16.0-alpha.3",
    "@ember-data/store": "^3.16.0-alpha.3",
    "ember-inflector": "^3.0.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/travis-cli": "^8.3.5",
    "@ember/optional-features": "^1.3.0",
    "@octokit/rest": "^17.0.1",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.14.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-htmlbars": "^4.2.3",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-lodash": "^4.17.5",
    "ember-qunit": "^4.6.0",
    "ember-qunit-assert-helpers": "^0.2.1",
    "ember-resolver": "^7.0.0",
    "ember-sinon": "^4.1.1",
    "ember-source": "~3.16.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-ember": "^7.8.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "loader.js": "^4.2.3",
    "moment": "^2.24.0",
    "pretender": "^3.3.1",
    "prettier": "^1.19.1",
    "qunit": "^2.9.3",
    "release-it": "^13.0.2",
    "release-it-lerna-changelog": "^2.0.0",
    "testem": "^3.0.2"
  },
  "peerDependencies": {
    "ember-data": ">= 3.8.0"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "defaultBlueprint": "schema",
    "after": [
      "ember-data",
      "@ember-data/debug"
    ]
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "changelog": {
    "ignoreCommitters": [
      "dependabot"
    ]
  },
  "volta": {
    "node": "13.9.0",
    "yarn": "1.12.1"
  }
}
